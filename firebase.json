{
  "hosting": {
    "cleanUrls": true,
    "trailingSlash": false,
    "headers": [
      { 
        "source": "/**", 
        "headers": [
          { "key": "Cache-Control", "value": "no-store" }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/**",
        "run": { 
          "serviceId": "admin-api", 
          "region": "australia-southeast1" 
        }
      }
    ]
  }
}
